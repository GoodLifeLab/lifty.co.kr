{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "pages/api/**/*": {
      "memory": 3009,
      "maxDuration": 300,
      "includeFiles": "**/*"
    }
  },
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "pnpm build"
}
